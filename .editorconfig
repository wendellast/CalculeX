#ect
root = true



# ALL
[*]
charset = utf-8
indent_style = tab
indent_size = 4
end_of_line = lf
insert_final_newline = true

# Python
[*.py]
indent_style = tab
indent_size = 4

# Markdown
[Makefile]
indent_style = tab

# JS
[lib/**.js]
indent_style = space
indent_size = 2

#package
[{package.json,.travis.yml}]
indent_style = space
indent_size = 2

#json
[*.json]
indent_style = tab
indent_size = 2
trim_trailing_whitespace = false
insert_final_newline = false

